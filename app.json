{
  "expo": {
    "name": "sommelai-app",
    "slug": "sommelai-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "sommelai-app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kristene1433.sommelai",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kristene1433.sommelai",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "expoRedirectUrl": "sommelai-app://stripe-redirect",
      "eas": {
        "projectId": "f2242574-94ff-4cbb-8d7f-28d1ffe48953"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
